<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:OpenXLive.WinRT.UI.Themes">
    
    <DataTemplate x:Key="SNSTemplate">
        <StackPanel Margin="0,10,10,0">
            <Image Source="{Binding Logo}" Width="60" Height="60" Stretch="UniformToFill"/>
        </StackPanel>
    </DataTemplate>

</ResourceDictionary>

